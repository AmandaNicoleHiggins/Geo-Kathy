  },
  "homepage": "https://github.com/Cassandraheil/Geo-Kathy#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors-anywhere": "^0.4.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "moment": "^2.25.3",
    "mongoose": "^5.9.13",
    "node": "^14.2.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
